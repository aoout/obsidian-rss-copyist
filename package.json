{
	"name": "rss-copyist",
	"version": "1.0.2",
	"description": "get the rss articles as mdfiles to your vault. no settingstab, config it in mdfiles.",
	"main": "lib/main.js",
	"license": "MIT",
	"scripts": {
		"build": "obsidian-plugin build src/main.ts",
		"dev": "node dev.js",
		"lint:fix": "eslint --ext .ts,.js,.json src --fix",
